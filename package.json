{
	"name": "crashd",
	"displayName": "CrasHD",
	"description": "Highlight source files with crash information generated by Zelos.",
	"version": "0.1.0",
	"license": "MIT",
	"publisher": "ZeropointDynamics",
	"author": {
		"name": "Ryan Court",
		"email": "rccourt@zeropointdynamics.com"
	},
	"activationEvents": [
		"onCommand:crashd.show",
		"onCommand:crashd.hide",
		"onCommand:crashd.reloadZcovFiles"
	],
	"engines": {
		"vscode": "^1.46.0"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "crashd.show",
				"title": "Show",
				"category": "CrasHD"
			},
			{
				"command": "crashd.hide",
				"title": "Hide",
				"category": "CrasHD"
			},
			{
				"command": "crashd.reloadZcovFiles",
				"title": "Reload (Load new .zcov File)",
				"category": "CrasHD"
			}
		],
		"configuration": [
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {}
}
